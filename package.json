{
  "name": "dose-segura",
  "description": "Dose Segura Application",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "homepage": "https://my-creations.github.io/dose-segura",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "predeploy": "npm run build:web && touch dist/.nojekyll && cp dist/index.html dist/404.html",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "lint:meds": "eslint data/meds.json",
    "lint:meds:fix": "eslint data/meds.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:single": "jest",
    "e2e": "playwright test -c e2e/playwright.config.ts",
    "e2e:ui": "playwright test -c e2e/playwright.config.ts --ui",
    "test:all": "npm run test && npm run e2e",
    "build:web": "expo export -p web",
    "build:android": "eas build -p android",
    "build:ios": "eas build -p ios"
  },
  "dependencies": {
    "@expo-google-fonts/quicksand": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-localization": "^17.0.8",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "i18n-js": "^4.5.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jsonc": "^2.21.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.4",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
